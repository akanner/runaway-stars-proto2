<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ParticipantResponse" table="participant-response">
    <indexes>
      <index name="fk_participant-response_session-participant_idx" columns="session-id"/>
      <index name="fk_participant-response_image1_idx" columns="first-image-served-id"/>
      <index name="fk_participant-response_image2_idx" columns="second-image-served-id"/>
      <index name="fk_participant-response_image3_idx" columns="third-image-served-id"/>
      <index name="fk_participant-response_image4_idx" columns="correct-image-id"/>
      <index name="fk_participant-response_image5_idx" columns="selected-image-id"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <many-to-one field="selectedImage" target-entity="Image">
      <join-columns>
        <join-column name="selected-image-id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="session" target-entity="ParticipantSession" inversed-by="responses">
      <join-columns>
        <join-column name="session-id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="correctImage" target-entity="Image">
      <join-columns>
        <join-column name="correct-image-id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="thirdImageServed" target-entity="Image">
      <join-columns>
        <join-column name="third-image-served-id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="secondImageServed" target-entity="Image">
      <join-columns>
        <join-column name="second-image-served-id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="firstImageServed" target-entity="Image">
      <join-columns>
        <join-column name="first-image-served-id" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
